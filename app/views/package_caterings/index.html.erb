<p id="notice"><%= notice %></p>

<h1>Listing Package Caterings</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Additional</th>
      <th>Package</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @package_caterings.each do |package_catering| %>
      <tr>
        <td><%= package_catering.price %></td>
        <td><%= package_catering.additional %></td>
        <td><%= package_catering.package %></td>
        <td><%= link_to 'Show', package_catering %></td>
        <td><%= link_to 'Edit', edit_package_catering_path(package_catering) %></td>
        <td><%= link_to 'Destroy', package_catering, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Package catering', new_package_catering_path %>
